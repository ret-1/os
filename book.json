{
    "plugins": [
        "hints",
        "callouts",
        "intopic-toc"
    ],
    "pluginsConfig": {
        "callouts": {
            "showTypeInHeader": false,
            "question_red": {
                "alert": "danger",
                "picto": "circle-question"
            },
            "question_blue": {
                "alert": "info",
                "picto": "circle-question"
            }
        },
        "intopic-toc": {
            "selector": ".markdown-section h2, .markdown-section h3"
        }
    }
}
